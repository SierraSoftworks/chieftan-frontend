{
  "name": "chieftan-frontend",
  "version": "1.0.0-beta.1.2.6",
  "description": "A frontend interface for managing a chieftan server.",
  "main": "dist/electron.js",
  "productName": "Chieftan",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --config build/webpack.production.js",
    "server:dev": "webpack-dev-server --config build/webpack.config.js --watch --hot",
    "server:prod": "http-server dist --cors",
    "watch": "npm run server:dev",
    "start": "npm run server:dev",
    "typings:prune": "typings prune"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@git.emss.co.za:chieftan/frontend.git"
  },
  "license": "CC0-1.0",
  "author": "Benjamin Pannell <bpannell@emss.co.za>",
  "homepage": "https://git.emss.co.za/chieftan/frontend",
  "aurelia": {
    "build": {
      "resources": [],
      "moduleRootOverride": {
        "aurelia-templating-resources": "dist/es2015",
        "aurelia-templating-router": "dist/es2015"
      }
    }
  },
  "dependencies": {
    "aurelia-bootstrapper-webpack": "^1.0.0-rc.1.0.0",
    "aurelia-event-aggregator": "^1.0.0-rc.1.0.0",
    "aurelia-fetch-client": "^1.0.0-rc.1.0.0",
    "aurelia-framework": "^1.0.0-rc.1.0.1",
    "aurelia-history-browser": "^1.0.0-rc.1.0.0",
    "aurelia-http-client": "^1.0.0-rc.1.0.0",
    "aurelia-loader-webpack": "^1.0.0-rc.1.0.0",
    "aurelia-logging-console": "^1.0.0-rc.1.0.0",
    "aurelia-pal-browser": "^1.0.0-rc.1.0.0",
    "aurelia-polyfills": "^1.0.0-rc.1.0.0",
    "aurelia-route-recognizer": "^1.0.0-rc.1.0.0",
    "aurelia-router": "^1.0.0-rc.1.0.0",
    "aurelia-templating-binding": "^1.0.0-rc.1.0.0",
    "aurelia-templating-resources": "^1.0.0-rc.1.0.0",
    "aurelia-templating-router": "^1.0.0-rc.1.0.0",
    "bluebird": "^3.4.1",
    "highlightjs": "^8.7.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.0.0",
    "materialize-css": "^0.97.6",
    "moment": "^2.14.1",
    "raven-js": "^3.2.1",
    "ts-helpers": "^1.1.1"
  },
  "devDependencies": {
    "aurelia-webpack-plugin": "1.0.0-beta.4.0.0",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "json-loader": "^0.5.4",
    "jsonfile": "^2.3.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.3",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "url-loader": "^0.5.7",
    "webpack": ">=2.1.0-beta.11 || ^2.1.0",
    "webpack-dev-server": ">=2.1.0-beta.0 || ^2.1.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^0.14.0"
  }
}
