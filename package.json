{
  "name": "chieftan-frontend",
  "version": "1.0.0-beta.1.2.6",
  "description": "A frontend interface for managing a chieftan server.",
  "main": "dist/electron.js",
  "productName": "Chieftan",
  "scripts": {
    "postinstall": "npm run typings:install",
    "test": "npm run node-babel -- ./node_modules/karma/bin/karma start config/karma.conf.js",
    "webdriver:update": "cross-env ./node_modules/.bin/webdriver-manager update",
    "webdriver:start": "cross-env ./node_modules/.bin/webdriver-manager start",
    "link:typescript": "npm run link:clean && shx ln -s typescript-src src && shx ln -s typescript-test test",
    "link:clean": "shx rm -f ./src ./test",
    "clean": "npm cache clean && cross-env ./node_modules/.bin/rimraf node_modules typings coverage dist",
    "clean:dist": "cross-env ./node_modules/.bin/rimraf dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "npm run webpack -- --config config/webpack.development.js",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "npm run webpack -- --config config/webpack.production.js",
    "postbuild:prod": "cp favicon.ico dist/favicon.ico",
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "npm run webpack-dev-server -- --config config/webpack.development.js --inline --watch",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:prod": "cross-env ./node_modules/.bin/http-server dist --cors",
    "node-babel": "node -r babel-register",
    "webpack": "npm run node-babel -- ./node_modules/webpack/bin/webpack",
    "webpack-dev-server": "npm run node-babel -- ./node_modules/webpack-dev-server/bin/webpack-dev-server",
    "typings:install": "cross-env ./node_modules/.bin/typings install",
    "typings:prune": "cross-env ./node_modules/.bin/typings prune"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@git.emss.co.za:chieftan/frontend.git"
  },
  "license": "CC0-1.0",
  "author": "Benjamin Pannell <bpannell@emss.co.za>",
  "homepage": "https://git.emss.co.za/chieftan/frontend",
  "aurelia": {
    "build": {
      "resources": [
        {
          "path": "users",
          "bundle": "users",
          "lazy": true
        }
      ],
      "moduleRootOverride": {
        "aurelia-templating-resources": "dist/es2015",
        "aurelia-templating-router": "dist/es2015"
      }
    }
  },
  "dependencies": {
    "aurelia-bootstrapper-webpack": "^1.0.0-beta.1.0.2",
    "aurelia-event-aggregator": "^1.0.0-beta.1.2.1",
    "aurelia-fetch-client": "^1.0.0-beta.1.2.5",
    "aurelia-framework": "^1.0.0-beta.1.2.5",
    "aurelia-history-browser": "^1.0.0-beta.1.2.1",
    "aurelia-http-client": "^1.0.0-beta.2.0.1",
    "aurelia-loader-webpack": "^1.0.0-beta.1.0.3",
    "aurelia-logging-console": "^1.0.0-beta.1.2.2",
    "aurelia-pal-browser": "^1.0.0-beta.2.0.1",
    "aurelia-polyfills": "^1.0.0-beta.1.1.6",
    "aurelia-route-recognizer": "^1.0.0-beta.1.2.1",
    "aurelia-router": "^1.0.0-beta.1.2.4",
    "aurelia-templating-binding": "^1.0.0-beta.1.2.4",
    "aurelia-templating-resources": "^1.0.0-beta.1.2.6",
    "aurelia-templating-router": "^1.0.0-beta.1.2.1",
    "bluebird": "^3.4.0",
    "highlightjs": "^8.7.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.0.0",
    "materialize-css": "^0.97.6",
    "moment": "^2.13.0",
    "regenerator-runtime": "^0.9.5",
    "ts-helpers": "^1.1.1"
  },
  "devDependencies": {
    "aurelia-tools": "^0.2.1",
    "aurelia-webpack-plugin": "^1.0.0-beta.2.0.0",
    "awesome-typescript-loader": "^1.0.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-es2015-loose-native-modules": "^1.0.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.9.0",
    "bundle-loader": "^0.5.4",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.20.0",
    "http-server": "^0.9.0",
    "inquirer": "^1.0.3",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "jsonfile": "^2.3.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "npm-run-all": "^2.1.2",
    "offline-plugin": "^3.3.1",
    "protractor": "^3.3.0",
    "raw-loader": "^0.5.1",
    "recursive-readdir-sync": "^1.0.6",
    "rimraf": "^2.5.2",
    "shx": "^0.1.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-babel-node-extendable": "^1.0.0",
    "typescript": ">=1.9.0-dev || ^2.0.0",
    "typings": "^1.0.4",
    "url-loader": "^0.5.7",
    "wallaby-webpack": "0.0.22",
    "webpack": ">=2.1.0-beta.11 || ^2.1.0",
    "webpack-dev-server": ">=2.1.0-beta.0 || ^2.1.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^0.14.0"
  },
  "language": "typescript"
}
