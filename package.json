{
  "name": "chieftan-frontend",
  "version": "1.0.0-beta.1.2.6",
  "description": "A frontend interface for managing a chieftan server.",
  "main": "dist/electron.js",
  "productName": "Chieftan",
  "scripts": {
    "clean": "npm cache clean && rimraf node_modules test/coverage dist",
    "clean:dist": "rimraf dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --progress --profile",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "npm run cross-env -- NODE_ENV=production npm run webpack",
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --inline --watch",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:prod": "http-server dist --cors",
    "cross-env": "node ./node_modules/cross-env/bin/cross-env.js",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@git.emss.co.za:chieftan/frontend.git"
  },
  "license": "CC0-1.0",
  "author": "Benjamin Pannell <bpannell@emss.co.za>",
  "homepage": "https://git.emss.co.za/chieftan/frontend",
  "aurelia": {
    "build": {
      "resources": []
    }
  },
  "dependencies": {
    "aurelia-bootstrapper-webpack": "^1.0.0",
    "aurelia-event-aggregator": "^1.0.0",
    "aurelia-fetch-client": "^1.0.0",
    "aurelia-framework": "^1.0.2",
    "aurelia-history-browser": "^1.0.0",
    "aurelia-http-client": "^1.0.0",
    "aurelia-loader-webpack": "^1.0.2",
    "aurelia-logging-console": "^1.0.0",
    "aurelia-pal-browser": "^1.0.0",
    "aurelia-polyfills": "^1.0.0",
    "aurelia-route-recognizer": "^1.0.0",
    "aurelia-router": "^1.0.2",
    "aurelia-templating-binding": "^1.0.0",
    "aurelia-templating-resources": "^1.0.0",
    "aurelia-templating-router": "^1.0.0",
    "bluebird": "^3.4.1",
    "highlightjs": "^8.7.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.0",
    "materialize-css": "^0.97.7",
    "moment": "^2.14.1",
    "raven-js": "^3.5.1"
  },
  "devDependencies": {
    "@easy-webpack/config-aurelia": "^2.0.1",
    "@easy-webpack/config-common-chunks-simple": "^2.0.1",
    "@easy-webpack/config-css": "^2.3.1",
    "@easy-webpack/config-env-development": "^2.1.1",
    "@easy-webpack/config-env-production": "^2.1.0",
    "@easy-webpack/config-external-source-maps": "^2.0.1",
    "@easy-webpack/config-fonts-and-images": "^1.2.1",
    "@easy-webpack/config-generate-index-html": "^2.0.1",
    "@easy-webpack/config-global-bluebird": "^1.2.0",
    "@easy-webpack/config-global-jquery": "^1.2.0",
    "@easy-webpack/config-global-regenerator": "^1.2.0",
    "@easy-webpack/config-html": "^2.0.2",
    "@easy-webpack/config-json": "^2.0.2",
    "@easy-webpack/config-less": "^1.1.1",
    "@easy-webpack/config-test-coverage-istanbul": "^2.0.2",
    "@easy-webpack/config-typescript": "^2.1.1",
    "@easy-webpack/config-uglify": "^2.1.0",
    "@easy-webpack/core": "^1.3.2",
    "@types/bluebird": "^2.0.29",
    "@types/whatwg-fetch": "0.0.27",
    "aurelia-tools": "^0.2.3",
    "concurrently": "^2.2.0",
    "cross-env": "^2.0.0",
    "http-server": "^0.9.0",
    "less": "^2.7.1",
    "rimraf": "^2.5.3",
    "ts-node": "^0.9.3",
    "typescript": "^2.0.0",
    "webpack": ">=2.1.0-beta.18 || ^2.1.0",
    "webpack-dev-server": ">=2.1.0-beta.0 || ^2.1.0"
  }
}
